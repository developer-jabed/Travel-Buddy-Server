{
  "name": "part-1",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "prisma": {
    "schema": "./prisma/schema/",
    "seed": "ts-node prisma/schema/seed.ts"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "build": "tsc",
    "start": "node ./dist/server.js",
    "db:generate": "prisma generate --schema=./prisma/schema",
    "db:migrate": "prisma migrate --schema=./prisma/schema",
    "db:push": "prisma db push --schema=./prisma/schema",
    "db:pull": "prisma db pull --schema=./prisma/schema",
    "db:studio": "prisma studio --schema=./prisma/schema",
    "postinstall": "prisma generate --schema=./prisma/schema",
    "stripe:webhook": "stripe listen --forward-to localhost:5000/webhook"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.10",
    "@types/nodemailer": "^6.4.17",
    "@types/socket.io": "^3.0.1",
    "@types/stripe": "^8.0.416",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "http-status": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.5",
    "prisma": "^6.11.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^19.3.1",
    "uuid": "^11.1.0",
    "zod": "^4.0.3"
  }
}
